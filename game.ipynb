    "    post_shoot_timeout_s=10.0,\n",
    "    countdown_step_s=0.55,\n",
    "    fps_smooth_every=10,\n",
    "    enable_voice=True,\n",
    "    save_results=True,            # True => save into results/<timestamp>/\n",
    "    results_root=\"results\",\n",
    "    password_enabled=False\n",
    ")\n",
    "\n",
    "\n",
    "game.run()\n"
